{"ast":null,"code":"import \"core-js/modules/es.string.trim.js\";\nimport Modal from '@/components/Modal.vue';\nexport default {\n  emits: ['save'],\n  components: {\n    Modal: Modal\n  },\n  data: function data() {\n    return {\n      showModal: false,\n      title: '',\n      content: '',\n      err: ''\n    };\n  },\n  methods: {\n    saveNote: function saveNote() {\n      if (!this.title.trim() && !this.content.trim()) {\n        this.err = 'Please fill in all fields';\n        return;\n      }\n\n      if (!this.title.trim()) {\n        this.err = 'Please fill in note title';\n        return;\n      }\n\n      if (!this.content.trim()) {\n        this.err = 'Please fill in note content';\n        return;\n      }\n\n      this.err = '';\n      this.showModal = false;\n      this.$emit('save', {\n        title: this.title.trim(),\n        content: this.content.trim()\n      });\n      this.title = '';\n      this.content = '';\n    }\n  }\n};","map":{"version":3,"mappings":";AAiCA,OAAOA,KAAP,MAAkB,wBAAlB;AAEA,eAAe;EACXC,KAAK,EAAE,CAAC,MAAD,CADI;EAEXC,UAAU,EAAE;IACRF,KAAI,EAAJA;EADQ,CAFD;EAKXG,IAAI,EAAE;IAAA,OAAO;MACTC,SAAS,EAAE,KADF;MAETC,KAAK,EAAE,EAFE;MAGTC,OAAO,EAAE,EAHA;MAITC,GAAG,EAAE;IAJI,CAAP;EAAA,CALK;EAWXC,OAAO,EAAE;IACLC,QADK,sBACM;MACP,IAAG,CAAC,KAAKJ,KAAL,CAAWK,IAAX,EAAD,IAAsB,CAAC,KAAKJ,OAAL,CAAaI,IAAb,EAA1B,EAA+C;QAC3C,KAAKH,GAAL,GAAW,2BAAX;QACA;MACJ;;MACA,IAAG,CAAC,KAAKF,KAAL,CAAWK,IAAX,EAAJ,EAAuB;QACnB,KAAKH,GAAL,GAAW,2BAAX;QACA;MACJ;;MACA,IAAG,CAAC,KAAKD,OAAL,CAAaI,IAAb,EAAJ,EAAyB;QACrB,KAAKH,GAAL,GAAW,6BAAX;QACA;MACJ;;MAEA,KAAKA,GAAL,GAAW,EAAX;MACA,KAAKH,SAAL,GAAiB,KAAjB;MAEA,KAAKO,KAAL,CAAW,MAAX,EAAmB;QACfN,KAAK,EAAE,KAAKA,KAAL,CAAWK,IAAX,EADQ;QAEfJ,OAAO,EAAE,KAAKA,OAAL,CAAaI,IAAb;MAFM,CAAnB;MAKA,KAAKL,KAAL,GAAa,EAAb;MACA,KAAKC,OAAL,GAAe,EAAf;IAEJ;EA1BK;AAXE,CAAf","names":["Modal","emits","components","data","showModal","title","content","err","methods","saveNote","trim","$emit"],"sourceRoot":"","sources":["/Users/jschack/notes/src/component